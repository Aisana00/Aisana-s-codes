class Shape:
    def area(self):
        """Print the area of the shape (0 by default)"""
        print(0)

class Square(Shape):
    def __init__(self, length):
        """Initialize Square with given length"""
        self.length = length
    
    def area(self):
        """Print the area of the square"""
        area_value = self.length * self.length
        print(area_value)